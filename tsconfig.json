{
  "compilerOptions": {
    "checkJs": true,
    "allowJs": true,
    "declaration": false,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "es2020",
    "target": "ES6"
  }
}
